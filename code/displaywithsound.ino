void setup() {
  // put your setup code here, to run once:
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C); //or 0x3C
  display.clearDisplay(); //for Clearing the display
  display.drawBitmap(0, 0, blank, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
  display.display();
  pinMode(buttonPin, INPUT); //initialize button pin 13 as input
  pinMode(plexPin, OUTPUT); //initialize quadplexer pin 5 as output
}

void loop() {
  // put your main code here, to run repeatedly:
  buttonState = digitalRead(buttonPin);
 //check if button is pressed
  if (buttonState == HIGH){
    display.clearDisplay(); //for Clearing the display
    display.drawBitmap(0, 0, SAMPLEantigen, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
    digitalWrite(plexPin, HIGH);
    display.display();
    delay(80);
    digitalWrite(plexPin, LOW);
    delay(1520);
    display.clearDisplay(); //for Clearing the display
    display.drawBitmap(0, 0, blank, 128, 64, WHITE); // display.drawBitmap(x position, y position, bitmap data, bitmap width, bitmap height, color)
    display.display();
  }
}
